{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
